# disable navigation bar
qemu.hw.mainkeys=1

# DPM
persist.vendor.dpm.feature=5

# DRM
drm.service.enabled=true

# Fingerprint
persist.qfp=false

# FRP
ro.frp.pst=/dev/block/bootdevice/by-name/frp

# GPS
persist.gps.qc_nlp_in_use=1
