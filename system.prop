ro.btconfig.dev=/dev/ttyHS0

# Camera
camera.disable_zsl_mode=1
persist.camera.gyro.disable=0
persist.camera.HAL3.enabled=1
persist.camera.imglib.fddsp=1
persist.camera.llc=1
